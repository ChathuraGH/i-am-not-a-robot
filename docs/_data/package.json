{
  "name": "i-am-not-a-robot",
  "version": "0.5.2",
  "description": "cause I can play games",
  "main": "i-am-not-a-robot.js",
  "scripts": {
    "browserify": "npm run browserify_demo; npm run browserify_dist; npm run minify",
    "browserify_demo": "NODE_PATH=. browserify -o docs/bundle.js demo.js",
    "browserify_dist": "browserify -r ./${npm_package_name}.js:${npm_package_name} -g [ envify --NODE_ENV production ] -g uglifyify -o dist/${npm_package_name}.js",
    "build": "npm run browserify; npm run docs",
    "docs": "run-s docs:*",
    "docs:package.json": "cp package.json docs/_data",
    "docs:homepage": "echo \"---\ntitle: $npm_package_name\n---\" > docs/index.md; cat README.md >> docs/index.md",
    "lint": "standa src/**/*.js",
    "minify": "cd dist; uglifyjs ${npm_package_name}.js --source-map --output ${npm_package_name}.min.js --compress --mangle -b beautify=false,preamble='\"// http://g14n.info/i-am-not-a-robot\"'; cd -",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_PATH=. budo --live --open --css docs/demo.css demo.js"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/i-am-not-a-robot.git"
  },
  "keywords": [
    "i",
    "am",
    "not",
    "robot",
    "anti",
    "spam",
    "antispam",
    "protection"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/i-am-not-a-robot/issues"
  },
  "homepage": "http://g14n.info/i-am-not-a-robot",
  "devDependencies": {
    "budo": "^11.3.2",
    "envify": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "npm-start-command": "^0.2.0",
    "pre-commit": "^1.2.2",
    "standa": "^2.1.0",
    "uglify-es": "^3.3.9",
    "uglifyify": "^5.0.1"
  },
  "dependencies": {}
}
